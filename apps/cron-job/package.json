{
  "name": "cron-job",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "npm run prisma:generate && npx tsc",
    "start": "npx prisma db push && node build",
    "prisma:generate" : "npx prisma generate"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "node-cron": "^3.0.3",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.1.7",
    "prisma": "^5.19.1",
    "ts-node": "^10.9.2"
  }
}
